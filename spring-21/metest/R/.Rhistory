ex <- al/be
sv <- sqrt(al/(be*be*n))
xx <- seq(0, 1, length.out = 1000)
yy <- dgamma(xx, al, be)
plot(xx, yy, type = "l", lwd = 2)
x_bar <- rep(NA, m)
for(t in 1:m){
x_bar[t] <- mean(rgamma(n, al, be))
}
# Si X ~ Gamma(a, b) , E(X) = a/b y Var(X) = a/b^2
